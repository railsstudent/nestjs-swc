    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "unplugin-swc": "1.3.2",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2",
    "rimraf": "5.0.1"
  }
